<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.5"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.5 ">
    <changeSet id="create_table_user" author="VMeloshnikov">
        <createTable tableName="USER">
            <column autoIncrement="true" name="user_id" type="bigint">
                ‹constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="first_name" type="varchar2">
                ‹constraints nullable="false"/>
            </column>
            <column name="last_name" type="varchar2">
                ‹constraints nullable="false"/>
            </column>
            <column name="email" type="varchar2">
                ‹constraints nullable="false" unique="true"/>
            </column>
            <column name="password" type="varchar2">
                ‹constraints nullable="false">
            </column>
        </createTable>
    </changeSet>


</databaseChangeLog>